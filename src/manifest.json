{
  "name": "Clean Bookmark",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2,
  "permissions": ["<all_urls>","tabs", "storage","bookmarks"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
